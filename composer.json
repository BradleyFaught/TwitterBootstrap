{
    "name": "slywalker/twitter_bootstrap",
    "type": "cakephp-plugin",
    "description": "Bootstrap (for Twitter) Plugin for CakePHP",
    "keywords": ["cakephp","bootstrap"],
    "homepage": "https://github.com/slywalker/TitterBootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Yasuo Harada",
            "email": "slywalker.net@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "bootstrap",
                "version": "v2.2.2",
                "source": {
                    "url": "git://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                }
            },
            "packagist": false
        }
    ],
    "require": {
        "bootstrap": "*",
        "composer/installers": "*"
    },
    "config": {
        "vendor-dir": "Vendor"
    }
}